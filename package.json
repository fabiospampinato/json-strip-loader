{
  "name": "json-strip-loader",
  "version": "1.0.4",
  "description": "JSON loader for Webpack, with support for keys stripping, useful in isomorphic builds for storing secrets in JSON files.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/json-strip-loader/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/json-strip-loader.git"
  },
  "keywords": [
    "webpack",
    "loader",
    "json",
    "strip",
    "isomorphic"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.117",
    "loader-utils": "^1.0.2",
    "lodash": "^4.17.11",
    "strip-keys": "^1.0.4"
  },
  "devDependencies": {
    "rimraf": "^2.6.0",
    "typescript": "~3.0.3",
    "typescript-transform-export-interop": "^1.0.0"
  }
}
